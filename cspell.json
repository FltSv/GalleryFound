{
  // スペルチェック除外リスト
  "words": ["galleryfound", "mypage"],
  "ignoreRegExpList": [
    "\\..*", // プロパティ名などを無視
    "/.*", // パス名などを無視
    "\bkey\b.*", // APIKeyなど、keyをSuffixとした変数以降の値を無視
    "\bid\b.*" // IDなどの値を無視
  ],
  "overrides": [
    {
      // TailwindCSSのカスタムフォント名を無視
      "filename": "**/*.{js,ts,jsx,tsx}",
      "ignoreRegExpList": ["font-.*"]
    }
  ],
  "dictionaries": ["google", "cpp", "flutter"]
}
